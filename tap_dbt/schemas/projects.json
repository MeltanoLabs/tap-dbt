          "type": "string"
        },
        "deploy_key_id": {
          "type": "integer"
        },
        "github_installation_id": {
          "type": "integer"
        },
        "state": {
          "type": "integer"
        },
        "created_at": {
          "type": "string"
        },
        "updated_at": {
          "type": "string"
        }
      }
    },
    "repository_id": {
      "type": "integer"
    },
    "state": {
      "type": "integer"
    },
    "created_at": {
      "type": "string"
    },
    "updated_at": {
      "type": "string"
    },
    "skipped_setup": {
      "type": "boolean"
    },
    "group_permissions": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "docs_job_id": {
      "type": ["integer", "null"]
    },
    "freshness_job_id": {
      "type": ["integer", "null"]
    },
    "docs_job": {
      "type": ["string", "null"]
    },
    "freshness_job": {
      "type": ["string", "null"]
    }
  }
